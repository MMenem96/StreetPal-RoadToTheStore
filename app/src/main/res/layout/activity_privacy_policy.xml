<?xml version="1.0" encoding="utf-8"?>


<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white">

    <RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/white"

        tools:context="com.sharekeg.streetpal.Settings.PrivacyPolicyActivity">


        <RelativeLayout
            android:id="@+id/TV_privacypolicy"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@color/login_button_bg"
            android:minHeight="40dp"
            android:padding="10dp">

            <ImageButton
                android:id="@+id/cancel_action"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_centerInParent="true"
                android:background="@drawable/ripple_effect"
                android:padding="5dp"
                android:src="@drawable/back_icon"
                android:textColor="@color/white" />

            <com.sharekeg.streetpal.TextViewForEnglish
                android:id="@+id/textViewForEnglish"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerHorizontal="true"
                android:layout_centerInParent="true"
                android:gravity="center"
                android:text="@string/privacypolicy"
                android:textAllCaps="true"
                android:textColor="@color/white"
                android:textSize="@dimen/activity_title_text_size" />


        </RelativeLayout>

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@id/TV_privacypolicy"
            android:paddingBottom="@dimen/activity_vertical_margin"
            android:paddingLeft="@dimen/activity_horizontal_margin"
            android:paddingRight="@dimen/activity_horizontal_margin"
            android:paddingTop="@dimen/activity_vertical_margin">

            <com.sharekeg.streetpal.TextViewForEnglish
                android:id="@+id/title"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                android:text="@string/streetPal"
                android:textColor="@color/black"
                android:textSize="15dp" />

            <com.sharekeg.streetpal.TextViewForEnglish
                android:id="@+id/Last"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/title"
                android:layout_marginTop="10dp"
                android:text="@string/Last"
                android:textColor="@color/black"
                android:textSize="12dp"

                />

            <com.sharekeg.streetpal.TextViewForEnglish
                android:id="@+id/by"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/Last"
                android:layout_marginTop="10dp"
                android:text="@string/by"
                android:textColor="@color/black"
                android:textSize="15dp" />

            <com.sharekeg.streetpal.TextViewForEnglish
                android:id="@+id/Subscriptions"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_below="@+id/by"
                android:layout_marginTop="10dp"
                android:text="@string/Subscriptions"
                android:textColor="@color/black"
                android:textSize="@dimen/privacy_title_text_size" />

            <com.sharekeg.streetpal.TextViewForEnglish
                android:id="@+id/Some"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/Subscriptions"
                android:layout_marginTop="10dp"
                android:text="@string/Some"
                android:textColor="@color/black"
                android:textSize="@dimen/privacy_text_size" />

            <com.sharekeg.streetpal.TextViewForEnglish
                android:id="@+id/Privacy"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_below="@id/Some"
                android:layout_marginTop="10dp"
                android:text="@string/Privacy"
                android:textColor="@color/black"
                android:textSize="@dimen/privacy_title_text_size" />

            <com.sharekeg.streetpal.TextViewForEnglish
                android:id="@+id/Personal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_below="@+id/Privacy"
                android:layout_marginTop="10dp"
                android:text="@string/Personal"
                android:textColor="@color/black"
                android:textSize="@dimen/privacy_title_text_size" />

            <com.sharekeg.streetpal.TextViewForEnglish
                android:id="@+id/contectOfSignUp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_below="@+id/Personal"
                android:layout_marginTop="10dp"
                android:text="@string/ContentOf"
                android:textColor="@color/black"
                android:textSize="@dimen/privacy_text_size" />

            <com.sharekeg.streetpal.TextViewForEnglish
                android:id="@+id/Content"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_below="@+id/contectOfSignUp"
                android:layout_marginTop="10dp"
                android:text="@string/Content"
                android:textColor="@color/black"
                android:textSize="@dimen/privacy_title_text_size" />

            <com.sharekeg.streetpal.TextViewForEnglish
                android:id="@+id/streetPal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_below="@id/Content"
                android:layout_marginTop="10dp"
                android:text="@string/STREET_PAL_des"
                android:textColor="@color/black"
                android:textSize="@dimen/privacy_text_size" />

            <com.sharekeg.streetpal.TextViewForEnglish
                android:id="@+id/Objectives"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_below="@+id/streetPal"
                android:layout_marginTop="10dp"
                android:text="@string/Objectives"
                android:textColor="@color/black"
                android:textSize="@dimen/privacy_title_text_size" />

            <com.sharekeg.streetpal.TextViewForEnglish
                android:id="@+id/increase"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/Objectives"
                android:layout_marginTop="10dp"
                android:text="@string/Building"
                android:textColor="@color/black"
                android:textSize="@dimen/privacy_text_size" />

            <com.sharekeg.streetpal.TextViewForEnglish
                android:id="@+id/Effective"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/increase"
                android:layout_marginTop="10dp"
                android:text="@string/Effective"
                android:textColor="@color/black"
                android:textSize="@dimen/privacy_title_text_size" />


            <com.sharekeg.streetpal.TextViewForEnglish
                android:id="@+id/MainFeatures"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_below="@+id/Effective"
                android:layout_marginTop="10dp"
                android:text="@string/mainfeatures"
                android:textColor="@color/black"
                android:textSize="@dimen/privacy_title_text_size" />

            <com.sharekeg.streetpal.TextViewForEnglish
                android:id="@+id/Smart"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/MainFeatures"
                android:layout_marginTop="10dp"
                android:text="@string/Smart"
                android:textColor="@color/black"

                android:textSize="@dimen/privacy_text_size" />

            <com.sharekeg.streetpal.TextViewForEnglish
                android:id="@+id/Navigation"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/Smart"
                android:text="@string/Navigation"
                android:textColor="@color/black"
                android:textSize="@dimen/privacy_text_size" />

            <com.sharekeg.streetpal.TextViewForEnglish
                android:id="@+id/Guide"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@id/Navigation"
                android:layout_marginTop="10dp"
                android:text="@string/Guide"
                android:textColor="@color/black"
                android:textSize="@dimen/privacy_text_size" />

            <com.sharekeg.streetpal.TextViewForEnglish
                android:id="@+id/ourService"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/Guide"
                android:layout_marginTop="10dp"
                android:text="@string/OurService"
                android:textColor="@color/black"
                android:textSize="@dimen/privacy_text_size" />

            <com.sharekeg.streetpal.TextViewForEnglish
                android:id="@+id/Usage"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_below="@id/ourService"
                android:layout_marginTop="10dp"
                android:text="@string/Usage"
                android:textColor="@color/black"
                android:textSize="@dimen/privacy_title_text_size" />

            <com.sharekeg.streetpal.TextViewForEnglish
                android:id="@+id/StreetPaloffers"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/Usage"
                android:layout_marginTop="10dp"
                android:text="@string/StreetPaloffers"
                android:textColor="@color/black"
                android:textSize="@dimen/privacy_text_size" />

            <com.sharekeg.streetpal.TextViewForEnglish
                android:id="@+id/Changes"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_below="@+id/StreetPaloffers"
                android:layout_marginTop="10dp"
                android:text="@string/Changes"
                android:textColor="@color/black"
                android:textSize="@dimen/privacy_title_text_size" />

            <com.sharekeg.streetpal.TextViewForEnglish
                android:id="@+id/We"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_below="@+id/Changes"
                android:layout_marginTop="10dp"
                android:text="@string/We"
                android:textColor="@color/black"
                android:textSize="@dimen/privacy_text_size" />

            <com.sharekeg.streetpal.TextViewForEnglish
                android:id="@+id/ContactUs"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_below="@id/We"
                android:layout_marginTop="10dp"
                android:text="@string/ContactUs"
                android:textColor="@color/black"
                android:textSize="@dimen/privacy_title_text_size" />

            <com.sharekeg.streetpal.TextViewForEnglish
                android:id="@+id/If"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentLeft="true"
                android:layout_below="@+id/ContactUs"
                android:layout_marginTop="10dp"
                android:text="@string/If"
                android:textColor="@color/black"
                android:textSize="@dimen/privacy_text_size" />

        </RelativeLayout>
    </RelativeLayout>
</ScrollView>
